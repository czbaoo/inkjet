  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gchudnov/inkjet/issues"
  },
  "homepage": "https://github.com/gchudnov/inkjet",
  "dependencies": {
    "debug": "^2.2.0",
    "imageinfo": "^1.0.4",
    "lodash.find": "^4.3.0",
    "webworkify": "^1.1.0"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^13.0.0",
    "gulp": "^3.9.1",
    "gulp-header": "^1.7.1",
    "gulp-load-plugins": "^1.2.2",
    "gulp-rename": "^1.2.2",
